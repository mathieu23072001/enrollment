
{% extends "baseSupAd.html.twig" %}

{% block stylesheets %} {{  parent() }} {% endblock %}

{% block title %} {{  parent() }} {% endblock %}


{% block body %}


 

  <!-- .row -->
               <div class="row">
                  <div class="col-md-12">
                     <div class="white-box">

{{ form_start(form,{'attr':{'class':'form-horizontal form-material'}}) }}

        <a href="javascript:void(0)" class="text-center db"><img src="{{ asset('optimum/logo.png')}}" alt="Home" /></a> 
        <h3 class="box-title m-t-40 m-b-0">Enregister un electeur</h3><small>Entrer les informations de l'electeur</small>
        
        <div class="col-sm-6 ol-md-6 col-xs-12">
                        <div class="white-box">
                            <h3 class="box-title"><center>Photo de l'electeur</center></h3>
                            <label for="input-file-now-custom-1"></label>
                            
                                {{form_widget(form.imageFile,{'attr':{'id':'input-file-now-custom-1'}})}}
                                
                        </div>
                    </div>


        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_widget(form.nom)}}

          </div>
        </div>
        <div class="form-group ">
          <div class="col-xs-12">
              
                                            {{form_widget(form.prenom)}}
          </div>
        </div>
        <div class="form-group ">
          <div class="col-xs-12">
              
                                            {{form_widget(form.pere)}}
          </div>
        </div>
        <div class="form-group">
          <div class="col-xs-12">
              
                                            {{form_widget(form.mere)}}
          </div>
        </div>

        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_row(form.dateNaissance)}}

          </div>
        </div>

        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_widget(form.lieuNaissance)}}

          </div>
        </div>

        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_widget(form.profession)}}

          </div>
        </div>

        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_widget(form.adresse)}}

          </div>
        </div>


        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_widget(form.sexe)}}

          </div>
        </div>

        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_widget(form.piece.nomCarte)}}

          </div>
        </div>

        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_widget(form.piece.numeroCarte)}}

          </div>
        </div>


        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_row(form.piece.dateCreation)}}

          </div>
        </div>


        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_row(form.piece.dateExpiration)}}

          </div>
        </div>

        <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_widget(form.carteElec.numeroForm)}}

          </div>
        </div>
        <div class="form-group">
          <div class="col-md-12">
            <div class="checkbox checkbox-primary p-t-0">
             
            </div>
          </div>
        </div>
        <div class="form-group text-center m-t-20">
          <div class="col-xs-12">
            <button class="btn btn-info btn-lg btn-block text-uppercase waves-effect waves-light"onclick="return confirm('Etes vous sure de vouloir enregistrer cet electeur?');" type="submit">Enregister</button>
          </div>
        </div>
        <div class="form-group m-b-0">
          <div class="col-sm-12 text-center">
            
          </div>
        </div>

      {{ form_end(form) }}



                     </div>
                  </div>
               </div>
               <!-- .row -->
	
					
					

 {% endblock %}

{% block javascript %} {{  parent() }} {% endblock %}
