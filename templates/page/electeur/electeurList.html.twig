{% extends "baseSupAd.html.twig" %}

{% block stylesheets %} {{  parent() }} {% endblock %}

{% block title %} {{  parent() }} {% endblock %}


{% block body %}

  {% if user %}

  <div class="row">
                    <div class="col-lg-12">
                      
                        <div class="white-box">
                            <h3 class="box-title">Editable with Datatable</h3>
                            <p class="text-muted">Just click on word which you want to change and enter</p>
                             <div class="white-box printableArea">
                            <table class="table table-striped table-bordered" id="">
                                <thead>
                                    <tr>
                                      <th>photo </th>
                                      <th>Numero de carte </th>
                                        <th>Nom </th>
                                        <th>Prenom </th>
                                        
                                        
                                        
                                    
                                        <th>Profession</th>
                                        <th>Adresse</th>
                                        <th>Date d'inscription</th>
                                        <th>Sexe</th>
                                        <th>Bureaux de vote</th>
                                        
                                        
                                        
                                    </tr>
                                </thead>
                                <tbody>
                                {% for electeur in electeurs %}
                                 {% for operat in user.operateurSaisis %}
                                 {% if electeur.crv == operat.crv  %}
                                 {% if electeur.rad == 1  %}
                                    <tr id="1" class="gradeX">
                                         <td> <img src="{{asset('images/electeurs/' ~ electeur.image)}}" alt="" width="80" height="80"></td>
                                         <td>{{electeur.carteElec.numeroCarte}}</td>
                                        <td>{{electeur.nom}}</td>
                                        <td>{{electeur.prenom}} </td>
                                        
                                        
                                        
                                        
                                        <td>{{electeur.profession}}</td>
                                         <td>{{electeur.adresse}}</td>
                                        <td>{{electeur.dateInscription|date("Y-m-d  H:i:s")}}</td>
                                     <td class="center">{{electeur.sexe}}</td>
                                        <td class="center">{{electeur.crv.nom}}</td>

                                      
                                        
                                    </tr>
                                     {% endif %}
                                     {% endif %}
                                     {% endfor %} 
                                   {% endfor %}
                                </tbody>
                                <tfoot>
                                    <tr>
                                      <th>photo </th>
                                      <th>Numero de carte </th>
                                    <th>Nom </th>
                                        <th>Prenom </th>
                                        
                                        
                                        
                                    
                                        <th>Profession</th>
                                        <th>Adresse</th>
                                        <th>Date d'inscription</th>
                                        <th>Sexe</th>
                                        <th>Bureaux de vote</th>
                                        
                                        
                                       
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                        </div>
                    </div>
                    <div class="text-right">
                                        
                                        <button id="print" class="btn btn-rounded btn-sm btn-info" type="button"> <span><i class="fa fa-print"></i> Print</span> </button>
                                    </div>
                </div>
                <!-- /.row -->


 {% endif %}

{% endblock %}

{% block javascript %} {{  parent() }} {% endblock %}
