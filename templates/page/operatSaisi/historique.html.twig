{% extends "baseSupAd.html.twig" %}

{% block stylesheets %} {{  parent() }} {% endblock %}

{% block title %} {{  parent() }} {% endblock %}


{% block body %}

{% if logs %}
  <div class="row">

  
                    <div class="col-lg-12">
                        <div class="white-box">
                            <h3 class="box-title">{{operat.user.email}}</h3>
                            <p class="text-muted">historiques de {{operat.nom}} {{operat.prenom}} </p>
                            <table class="table table-striped table-bordered" id="editable-datatable">
                                <thead>
                                    <tr>
                                        <th>Libelle</th>
                                        <th>Date execution</th>
                                        
                                    </tr>
                                </thead>
                                <tbody>
                                {% for log in logs %}
                                    <tr id="1" class="gradeX">
                                        <td>{{log.libelle}}</td>
                                        
                                        
                                        
                                        <td class="center">{{log.CreatAt|date("Y-m-d  H:i:s")}}</td>
                                        
                                         
                                        
                                        
                                    </tr>
                                   {% endfor %}
                                </tbody>
                                <tfoot>
                                    <tr>
                                       <th>libelle</th>
                                        <th>Date execution</th>
                                      
                                        
                                    </tr>
                                </tfoot>
                            </table>
                        </div>
                    </div>
  {{ form_start(form,{'attr':{'class':'form-horizontal'}}) }}
                    <div class="form-group m-t-20">
          <div class="col-xs-12">
              
                                            {{form_widget(form.numeroCarte)}}
                                             

          </div>

           <div class="col-sm-offset-3 col-sm-5">
                                  <button type="submit" class="btn btn-info btn-rounded btn-sm" > <i class="fa fa-plus"></i>&nbsp;&nbsp;Rechercher</button>
                            </div>
                </div>
                 {{ form_end(form) }}
                 </div>
                <!-- /.row -->
{% else %}
<h3 class="box-title">Cet n'a effectue aucune action</h3>
{% endif %}


{% endblock %}

{% block javascript %} {{  parent() }} {% endblock %}
